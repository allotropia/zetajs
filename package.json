    "zetaoffice",
    "allotropia",
    "collabora",
    "wasm",
    "lowa",
    "typescript"
  ],
  "author": "allotropia software GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allotropia/zetajs/issues"
  },
  "homepage": "https://github.com/allotropia/zetajs#readme",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "puppeteer": "^23.11.1",
    "typescript": "^5.8.2"
  }
