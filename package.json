{
  "name": "zetajs",
  "version": "1.2.0",
  "description": "Access ZetaOffice in the Browser from JavaScript via UNO",
  "exports": {
    "./zeta.js": "./source/zeta.js",
    "./zetaHelper.js": "./source/zetaHelper.js",
    "./zetaHelper.ts": "./source/zetaHelper.ts"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "files": [
    "/CHANGELOG.md",
    "/LICENSE",
    "/source/zeta.js",
    "/source/zetaHelper.js"
  ],
  "scripts": {
